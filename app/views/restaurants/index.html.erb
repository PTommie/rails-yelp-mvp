<h1> Restaurants </h1>

<% @restaurants.each do |restaurant| %>
      <ul>
        <li><%= restaurant.name %> - <%= link_to "details", restaurant_path(restaurant) %></li>
        <li><%= restaurant.address %></li>
        <li><%= restaurant.phone_number %></li>
        <li><%= restaurant.category %></li>
       <!-- <li><%= restaurant.reviews %></li> -->
      </ul>
    <% end %>

